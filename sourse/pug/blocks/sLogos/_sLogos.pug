mixin sLogos(data)
	// start sLogos
	+b.SECTION.sLogos.section#sLogos&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 6 000+ компаний
				p  уже доверяют нам
				
			+e.row.row
				- for (var x = 1; x < 16; x++)
					+e.col.col-lg.col-md-3.col-sm-4.col-6
						+img(`img/logo-${x}.jpg`)
	// end sLogos